#%RAML 1.0 DataType

type: object
properties:
      firstName:
        example: testName
        type: string
      emailId:
        example: test@mail.com
        type: string
      lastName:
        example: testLastName
        type: string
      customerId:
        example: 1
        type: number
        format: int
      phoneNumber:
        example: "+91999999999"
        type: string